{
  extends: [
    "config:base",
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        "(version|ref): \"?(?<currentValue>.*)\"? +# renovate: depName=(?<depName>.*)",
        "name: \"?(?<depName>.*)@(?<currentValue>.*)\"?"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
