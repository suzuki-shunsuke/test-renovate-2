{
  "extends": [
    "github>suzuki-shunsuke/renovate-config:golang"
  ],
  "regexManagers": [
    {
      "fileMatch": ["aqua.yaml"],
      "matchStrings": [
        "version: (?<currentValue>.*?) # renovate: depName=(?<depName>.*?)\\n"
      ],
      "datasourceTemplate": "github-releases"
    },
    {
      "fileMatch": ["tutorial/docker-compose.yml"],
      "depNameTemplate": "suzuki-shunsuke/aqua",
      "datasourceTemplate": "github-releases",
      "matchStrings": [
        "aqua_version: (?<currentValue>.*)"
      ]
    },
    {
      "fileMatch": ["tutorial/docker-compose.yml"],
      "depNameTemplate": "suzuki-shunsuke/aqua-installer",
      "datasourceTemplate": "github-releases",
      "matchStrings": [
        "aqua_installer_version: (?<currentValue>.*)"
      ]
    },
    {
      "fileMatch": [".*\\.go"],
      "depNameTemplate": "suzuki-shunsuke/aqua-proxy",
      "datasourceTemplate": "github-releases",
      "matchStrings": [
        '"(?<currentValue>.*)", // renovate: depName=suzuki-shunsuke/aqua-proxy'
      ]
    }
  ]
}
