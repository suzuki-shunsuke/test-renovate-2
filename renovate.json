{
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "regexManagers": [
    {
      "fileMatch": ["scripts/.*\\.sh"],
      "matchStrings": [
        "aqua-installer +\\| +(ba|z)?sh -s -- -v ['\"]?(?<currentValue>[^'\" \\n]+)['\"]?\\s"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "aquaproj/aqua"
    }
  ]
}
