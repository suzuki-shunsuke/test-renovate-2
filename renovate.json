{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        "^ +(version|ref): (?<currentValue>[^'\"\\n]*) +# renovate: depName=(?<depName>.*)",
        "^ +(version|ref): \"(?<currentValue>[^'\"\\n]*)\" +# renovate: depName=(?<depName>.*)",
        "^ +(version|ref): '(?<currentValue>[^'\"\\n]*)' +# renovate: depName=(?<depName>.*)",
        "name: (?<depName>[^'\"]*)@(?<currentValue>[^'\"\\n]*)",
        "name: \"(?<depName>[^']*)@(?<currentValue>[^'\"\\n]*)\"",
        "name: '(?<depName>[^\"]*)@(?<currentValue>[^'\"\\n]*)'"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
