{
  "labels": [
    "renovate:{{depName}}",
    "renovate:{{depName}}/{{newVersion}}"
  ],
  "regexManagers": [
    {
      "fileMatch": [".akoi.yml"],
      "matchStrings": ["version: (?<currentValue>.*?)  # renovate: depName=(?<depName>.*?)\\n"],
      "datasourceTemplate": "github-releases",
      "schedule": [
        "at 15:00 am on Thurs"
      ]
    }
  ]
}
